{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "no-unused-vars": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 1,
    "no-restricted-syntax": 0,
    "react/react-in-jsx-scope": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
  }
}